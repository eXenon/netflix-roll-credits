{
  "name": "Netflix - Roll Credits",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Let credits roll at the end of Netflix movies",
  "homepage_url": "https://github.com/eXenon/netflix-roll-credits",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.netflix.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}